char *my_strcat(char* dest, char* str)
{
	char* ret = dest;
	assert(dest != NULL);
	assert(str);
	while (*dest!='\0')
	{
		dest++;

	}
	while (*dest++ = *str++)
	{
		;
	}
	return ret;
}
//int main()
//{
