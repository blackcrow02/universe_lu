#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<assert.h>
char* my_strcpy(char* p,const char* p1)
{
	assert(p1 != NULL);
	assert(p != NULL);
	char* ret = p;
	while (*p++ = *p1++)
	{
		;
	}
	return ret;
}
//int main()
