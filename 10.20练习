//	printf("%s", arr);
//	return 0;//追加
//}
int my_strcmp(const char* str, const char* str1)
{
	assert(str && str1);
	while (*str == *str1)
	{
		if (*str =='/0')
		{
			return 0;
		}
		str++;
		str1++;
	}
	   
	return (*str-*str1);

	
}
int main()
{
	char* arr = "ab";
	char* arr1 = "abcd";
	int ret =my_strcmp(arr, arr1);
	printf("%d", ret);
	return 0;//比大小
}

	
