int main()
{
	float time, size, spend;
	printf("输入下载速度");
	scanf("%f", &spend);
	printf("输入文件大小");
	scanf("%f", &size);
	time = size * 8/spend;
	printf("下载大小：%f", time);
	return 0;

}

